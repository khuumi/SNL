recipe fib to x:
  start fib:
    y is x
    if x < 2
      (return 1)
    else
      (return (do fib to x -2) + (do fib to y -1))
  done
done

start main:
  do show to do fib to 0
  do show to ""
  do show to do fib to 1
  do show to ""
  do show to do fib to 2
  do show to ""
  do show to do fib to 3
  do show to ""
  do show to do fib to 4
  do show to ""
  do show to do fib to 5
  do show to ""
  do show to do fib to 10
done
